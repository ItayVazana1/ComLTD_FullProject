{
    "test_cases": [
        {
            "name": "Register User 1",
            "url": "http://localhost:10000/users/register",
            "method": "POST",
            "data": {
                "full_name": "John Doe",
                "username": "john_doe",
                "email": "john@example.com",
                "phone_number": "1234567890",
                "password": "Password123",
                "confirm_password": "Password123",
                "accept_terms": true,
                "gender": "male"
            },
            "expected_status": 200
        },
        {
            "name": "Login User 1",
            "url": "http://localhost:10000/users/login",
            "method": "POST",
            "data": {
                "username_or_email": "john_doe",
                "password": "Password123",
                "remember_me": false
            },
            "expected_status": 200
        },
        {
            "name": "Update User 1",
            "url": "http://localhost:10000/users/{user_id}",
            "method": "PUT",
            "data": {
                "full_name": "Johnathan Doe",
                "phone_number": "1112223333",
                "email": "johnathan@example.com",
                "gender": "male"
            },
            "expected_status": 200,
            "dependency": "Register User 1"
        }
    ]
}
